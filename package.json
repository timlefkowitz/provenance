{
  "name": "provence-app",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "dev": "pnpm with-env next dev --turbo | pino-pretty -c",
    "build": "next build",
    "build:local": "pnpm with-env next build",
    "start": "next start",
    "start:local": "pnpm with-env next start",
    "lint": "eslint .",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ./.env.local --"
  },
  "dependencies": {
    "@kit/accounts": "workspace:*",
    "@kit/auth": "workspace:*",
    "@kit/i18n": "workspace:*",
    "@kit/next": "workspace:*",
    "@kit/supabase": "workspace:*",
    "@kit/ui": "workspace:*",
    "@supabase/supabase-js": "2.75.0",
    "@tanstack/react-query": "5.90.2",
    "@vercel/analytics": "^1.6.1",
    "exifr": "^7.1.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.9",
    "next-themes": "0.4.6",
    "nodemailer": "^7.0.12",
    "qrcode.react": "^4.2.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-i18next": "^16.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.32"
